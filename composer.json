{
  "name": "netlife/craft-starter",
  "description": "Good defaults for Craft CMS 4",
  "license": "proprietary",
  "require": {
    "craftcms/ckeditor": "3.8.3",
    "craftcms/cms": "4.8.9",
    "nystudio107/craft-seomatic": "4.0.48",
    "nystudio107/craft-vite": "4.0.9",
    "verbb/hyper": "1.1.27",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3.2"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
