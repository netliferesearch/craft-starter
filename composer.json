{
  "name": "netlife/craft-starter",
  "description": "Good defaults for Craft CMS 5",
  "license": "proprietary",
  "require": {
    "craftcms/ckeditor": "4.0.3",
    "craftcms/cms": "^5.0.0",
    "nystudio107/craft-seomatic": "5.0.2",
    "nystudio107/craft-vite": "5.0.0",
    "verbb/hyper": "2.0.0-beta.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
