{
  "name": "netlife/craft-starter",
  "description": "Good defaults for Craft CMS 3",
  "license": "proprietary",
  "require": {
    "craftcms/cms": "^4.0.0",
    "craftcms/redactor": "3.0.2",
    "nystudio107/craft-seomatic": "4.0.7",
    "nystudio107/craft-vite": "4.0.2",
    "sebastianlenz/linkfield": "2.1.3-rc",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    }
  }
}