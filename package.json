{
  "name": "netlife-craft-starter",
  "version": "3.0.0",
  "description": "Good defaults for Craft CMS 3",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "echo 'You probably want to $ npm run dev'",
    "dev": "webpack --watch --devtool eval-source-map",
    "build": "webpack -p --devtool source-map --progress",
    "postinstall": "npm run build",
    "heroku-postbuild": "mkdir public/admin && cp -R craft/app/resources public/admin",
    "db:prod:download": "hamstr mysqldump --tunnel -p docker-src/db-dump/dump.sql prod",
    "db:stage:push": "echo 'command not implemented'",
    "docker:rebuild": "docker-compose down && docker-compose up",
    "clean": "rimraf .cache ./web/dist"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "author": "Netlife Design",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "hamstr": "^1.0.3",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "babel-loader": "^8.0.0-beta.0",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {}
    }
  }
}
